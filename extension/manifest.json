{
  "name": "Get pages source",
  "version": "1.1",
  "manifest_version": 3,
  "description": "Get active tabs or element on that pages source from a popup",

  "action": {
      "default_title": "Get pages source",
      "default_popup": "popup.html",
      "default_icon": "logo.png"
  },

  "web_accessible_resources": [
    {
      "resources": [
        "onnxruntime-web/dist/ort.min.js",
        "onnxruntime-web/dist/ort-wasm-simd.wasm",
        "onnxruntime-web/dist/*",
        "content2.js"
      ],
      "matches": ["<all_urls>"]
    }
  ],

  "host_permissions": ["https://twitter.com/", "https://x.com/"],

  "content_scripts": [
    {
        "matches": ["https://twitter.com/", "https://x.com/"],
        "js": ["onnxruntime-web/dist/ort.min.js", "./content2.js"]
    }
],

  "permissions": ["scripting", "activeTab", "tabs"],
  "content_security_policy": {
    "extension_pages": "script-src 'wasm-unsafe-eval' 'self'; script-src-elem blob: 'unsafe-eval' 'unsafe-inline' 'wasm-unsafe-eval' 'self'; object-src 'self'"
  }
}
 




